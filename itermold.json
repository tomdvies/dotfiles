{
"title": "New iTerm2 Window",
"author": "Ahnberg <ahnberg@netnod.se>",
"rules": [ {
        "description": "New iTerm2 Window",
        "manipulators": [ {
                "type": "basic",
                "from": { "key_code": "c",
                          "modifiers": { "mandatory": [ "command", "option","control","shift" ] } },
                "to": [ { "shell_command": "osascript ~/applescripts/iterm.scpt &>~/out.txt" } ]
            } ]
    } ]
}
